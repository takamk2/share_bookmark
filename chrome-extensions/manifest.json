{
    "name": "元気が出るスイッチ",
    "manifest_version": 3,
    "version": "0.1",
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "http://localhost/"
    ],
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png"
        },
        "default_popup": "popup.html"
    }
}
